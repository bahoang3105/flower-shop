{
  "name": "nextjs-blog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:staging": "env-cmd -f .env.staging next dev",
    "build:dev": "env-cmd -f .env.development next build",
    "build:staging": "env-cmd -f .env.staging next build",
    "build:prod": "env-cmd -f .env.production next build",
    "start:dev": "env-cmd -f .env.development next start",
    "start:staging": "env-cmd -f .env.staging next start",
    "start:prod": "env-cmd -f .env.production next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "prettier:check": "prettier --write .prettierrc.js --check \"*.{ts,js,tsx,jsx,scss,css,html}\""
  },
  "lint-staged": {
    "*.{ts,js,tsx,jsx}": [
      "eslint --fix",
      "npm run prettier:check",
      "git add ."
    ]
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.0",
    "@zeit/next-sass": "^1.0.1",
    "antd": "^4.18.9",
    "classnames": "^2.3.1",
    "lodash": "^4.17.21",
    "next": "12.1.0",
    "next-compose-plugins": "^2.2.1",
    "next-i18next": "^10.5.0",
    "next-images": "^1.8.4",
    "next-redux-wrapper": "^7.0.5",
    "next-seo": "^5.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-query": "^3.39.2",
    "react-redux": "^7.2.6",
    "redux": "^4.1.2",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "sass": "^1.49.9"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.179",
    "@types/node": "^17.0.21",
    "@types/react": "18.0.1",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "8.10.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "12.1.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "lint-staged": "^12.3.4",
    "prettier": "^2.2.1",
    "typescript": "^4.6.2"
  }
}
